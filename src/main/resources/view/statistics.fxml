<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="1500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="controller.StatisticsController">
    <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0" style="-fx-padding: 20;"
                stylesheets="@../style/copyable-text.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox>
            <TextField style="-fx-font-weight: bold;-fx-font-size: 18" styleClass="copyable-label" text="Report"/>
            <TextField styleClass="copyable-label" text="date: $date"/>
            <Separator prefWidth="200.0"/>
            <TextField layoutX="10.0" layoutY="374.0" style="-fx-font-weight: bold" styleClass="copyable-label"
                       text="statistics"/>
            <FlowPane>

            </FlowPane>
            <TextField layoutX="10.0" layoutY="97.0" style="-fx-font-weight: bold" styleClass="copyable-label"
                       text="result"/>
            <TextField style="-fx-font-weight: bold" styleClass="copyable-label" text="preferences"/>
            <GridPane>
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <TextField styleClass="copyable-label" text="extend of busy: $extend ($desc)" GridPane.rowIndex="1"/>
                <TextField styleClass="copyable-label" text="range of goods quantity: from $from to $to"
                           GridPane.columnSpan="2" GridPane.rowIndex="3"/>
                <TextField layoutX="10.0" layoutY="37.0" styleClass="copyable-label" text="general"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label" text="customer"
                           GridPane.rowIndex="2"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label" text="general"/>
                <TextField layoutX="10.0" layoutY="97.0" styleClass="copyable-label"
                           text="percentage of a customer who can not wait: $p" GridPane.columnIndex="1"
                           GridPane.columnSpan="2" GridPane.rowIndex="3"/>
                <TextField layoutX="390.0" layoutY="97.0" styleClass="copyable-label"
                           text="customer will leave after waiting for: $t mins" GridPane.columnSpan="2"
                           GridPane.rowIndex="4"/>
                <TextField layoutX="10.0" layoutY="127.0" styleClass="copyable-label" text="checkout"
                           GridPane.rowIndex="5"/>
                <TextField layoutX="10.0" layoutY="157.0" styleClass="copyable-label"
                           text="quantity of normal checkouts: $q" GridPane.rowIndex="6"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label"
                           text="quantity of expressway checkouts: $q" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label"
                           text="expressway checkouts for products less than %n" GridPane.rowIndex="7"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label"
                           text="range of each product scan time: from $from to $to" GridPane.columnIndex="1"
                           GridPane.rowIndex="7"/>
            </GridPane>
        </VBox>
    </ScrollPane>
</AnchorPane>
