<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="1500.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="controller.StatisticsController"
            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
    <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0" style="-fx-padding: 20;"
                stylesheets="@../style/copyable-text.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" fx:id="ReportPane">
        <VBox>
            <TextField style="-fx-font-weight: bold;-fx-font-size: 18" styleClass="copyable-label" text="Report"/>
            <TextField fx:id="date" styleClass="copyable-label"/>
            <GridPane>
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    <ColumnConstraints/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <TextField fx:id="extendOfBusy" styleClass="copyable-label" GridPane.rowIndex="1"/>
                <TextField fx:id="rangeOfGoodsQuantity" styleClass="copyable-label"
                           GridPane.columnSpan="2" GridPane.rowIndex="3"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label" text="customer"
                           style="-fx-font-weight: bold"
                           GridPane.rowIndex="2"/>
                <TextField layoutX="10.0" layoutY="7.0" styleClass="copyable-label" text="general"
                           style="-fx-font-weight: bold"/>
                <TextField fx:id="percentageOfACustomerCanNotWait" layoutX="10.0" layoutY="97.0"
                           styleClass="copyable-label"
                           GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3"/>
                <TextField fx:id="CustomerWillLeaveAfter" layoutX="390.0" layoutY="97.0" styleClass="copyable-label"
                           GridPane.columnSpan="2" GridPane.rowIndex="4"/>
                <TextField layoutX="10.0" layoutY="127.0" styleClass="copyable-label" text="checkout"
                           style="-fx-font-weight: bold"
                           GridPane.rowIndex="5"/>
                <TextField fx:id="quantityOfNormalCheckouts" layoutX="10.0" layoutY="157.0" styleClass="copyable-label"
                           GridPane.rowIndex="6"/>
                <TextField fx:id="quantityOfExpresswayCheckouts" layoutX="10.0" layoutY="7.0"
                           styleClass="copyable-label"
                           GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                <TextField fx:id="ExpresswayCheckoutsFor" layoutX="10.0" layoutY="7.0" styleClass="copyable-label"
                           GridPane.rowIndex="7"/>
                <TextField fx:id="rangeOfScanTime" layoutX="10.0" layoutY="7.0" styleClass="copyable-label"
                           GridPane.columnIndex="1" GridPane.rowIndex="7"/>
            </GridPane>
            <Separator prefWidth="200.0"/>
            <TextField layoutX="10.0" layoutY="374.0" style="-fx-font-weight: bold" styleClass="copyable-label"
                       text="statistics"/>
            <FlowPane>
                <PieChart fx:id="waitTimeDistributionPie"/>
                <ScatterChart fx:id="waitTimeEachCustomerScatter">
                    <xAxis>
                        <NumberAxis side="BOTTOM"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT"/>
                    </yAxis>
                </ScatterChart>
                <BarChart fx:id="utilizationEachCheckoutBar">
                    <xAxis>
                        <CategoryAxis side="BOTTOM"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT"/>
                    </yAxis>
                </BarChart>
                <LineChart fx:id="totalProductionProcessedLine">
                    <xAxis>
                        <NumberAxis side="BOTTOM"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis side="LEFT"/>
                    </yAxis>
                </LineChart>
            </FlowPane>
            <TextField layoutX="10.0" layoutY="97.0" style="-fx-font-weight: bold" styleClass="copyable-label"
                       text="record"/>
            <TextArea fx:id="recordDetail" styleClass="copyable-label"/>
        </VBox>
    </ScrollPane>
</AnchorPane>
